{
  "version": 2,
  "builds": [
    {
      "src": "requirements_lab/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "requirements_lab/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "PYTHONDONTWRITEBYTECODE": "1",
    "PYTHONUNBUFFERED": "1"
  },
  "functions": {
    "requirements_lab/wsgi.py": {
      "runtime": "python3.9"
    }
  }
}